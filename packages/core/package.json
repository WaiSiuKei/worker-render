{
  "name": "@neditor/core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "gen:js": "pbjs -t static-module -w es6 -o src/common/proto/person.js  src/common/proto/person.proto  --no-delimited --no-verify --no-typeurl --es6",
    "gen:ts": "pbts -o src/common/proto/person.d.ts src/common/proto/person.js",
    "gen": "npm run gen:js && npm run gen:ts"
  },
  "dependencies": {
    "normalize.css": "8",
    "debug": "4.3.4",
    "@neditor/core": "workspace:*",
    "protobufjs": "7.2.5"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "typescript": "5.2.2",
    "vite": "3.2.4",
    "vite-plugin-checker": "0.4.8"
  },
  "exports": {
    ".": {
      "import": {
        "development": "./src",
        "default": "./dist/es/index.mjs"
      }
    },
    "./*": {
      "import": {
        "development": "./src/*",
        "default": "./dist/es/*.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ]
    }
  }
}
